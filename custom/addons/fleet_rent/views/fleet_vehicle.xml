            <xpath expr="//field[@name='license_plate']" position="replace" >
                <div class="oe_title">
                    <group colspan="2" col="2">
                        <label for="name_seq"/>
                        <h5>
                            <field name="name_seq" class="oe_inline"/>
                        </h5>
                        <label for="f_brand_id"/>
                        <h5>
                            <field name="f_brand_id" class="oe_inline"/>
                        </h5>
<!--                        <label for="model_no"/>-->
<!--                        <h5>-->
<!--                            <field name="model_no" class="oe_inline"/>-->
<!--                        </h5>-->
                        <label for="license_plate"/>
                        <h5>
                            <field name="license_plate" class="oe_inline" placeholder="e.g. PAE 326"/>
                        </h5>
                        <field name="current_user_admin" invisible="1" />
                        <label for="name"/>
                        <h5>
                            <field name="name" class="oe_inline" attrs="{'readonly': [('current_user_admin','=',False)]}" />
                        </h5>
                    </group>
                </div>
            </xpath>
<!--            <xpath expr="//group/group[1]" position="attributes">-->
<!--                <attribute name="attrs">{'invisible': [True]}</attribute>-->
<!--            </xpath>-->
            <xpath expr="//group[2]" position="replace">
                <group colspan="2" col="2" string="General Properties">
                    <group>
                        <field name="driver_id" domain="[('is_driver', '=', True)]" invisible="1"/>
                        <field name="submitted_registration" invisible="1"/>
                        <field name="finished_registration" invisible="1"/>
                        <field name="ready_for_approval" invisible="1"/>
                        <field name="vehicle_prodcut_template_id" invisible="1"/>
                        <field name="vehicle_cost_price" />
                        <field name="vin_sn"/>
                        <field name="vehicle_type_image" widget="image"/>
                        <field name="company_id" groups="base.group_multi_company"/>
                    </group>
                    <group>
                        <label for="odometer"/>
                        <div class="o_row">
                            <field name="odometer" required="1"/>
                            <field name="odometer_unit"/>
                        </div>
                        <field name="engine_no" attrs="{'required': [('state_id', '!=', 7)]}"/>
                        <field name="released_date" />
                        <field name="reg_id" />
                        <field name="acquisition_date" invisible="1"/>
                        <label for="warranty_period"/>
                        <div class="o_row">
                            <field name="warranty_period"/>
                            <field name="warranty_period_km" placeholder='In KM'/>
                        </div>
                        <field name="service_interval" string="Service Due (In km)"/>
                        <field name="income_acc_id" />
                        <field name="expence_acc_id"/>
                        <field name="state" invisible="1"/>
                        <field name="active_contract" attrs="{'invisible': [('state','!=','rent')]}" readonly="1"/>
                    </group>
                </group>
            </xpath>
