        <!-- Pre Registration Location -->
        <record id="fleet_pre_location" model="stock.location">
            <field name="name">Pre Registration Location</field>
            <field name="location_id" model="stock.location" eval="obj().env.ref('stock.warehouse0').view_location_id.id"/>
            <field name="usage">internal</field>
        </record>

        <record id="fleet_pre_location_operation_type" model="stock.picking.type">
            <field name="name">Pre Registration Receiving</field>
            <field name="sequence_id">5</field>
            <field name="sequence_code">IN</field>
            <field name="code">incoming</field>
            <field name="show_operations">0</field>
            <field name="default_location_dest_id" ref="fleet_pre_location"/>
        </record>

