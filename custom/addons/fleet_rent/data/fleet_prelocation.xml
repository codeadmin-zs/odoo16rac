    <data noupdate="1">
        <record id="fleet_rental.vehicle_state_inactive" model="fleet.vehicle.state">
            <field name="name">Inactive</field>
            <field name="sequence">1</field>
        </record>
        <record id="fleet.fleet_vehicle_state_registered" model="fleet.vehicle.state">
            <field name="name">Registered</field>
            <field name="sequence">2</field>
        </record>
