<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="external_layout_standard_contract">
        <t t-if="not o" t-set="o" t-value="doc"/>

        <t t-if="not company">
            <!-- Multicompany -->
            <t t-if="company_id">
                <t t-set="company" t-value="company_id"/>
            </t>
            <t t-elif="o and 'company_id' in o">
                <t t-set="company" t-value="docs.company_id.sudo()"/>
                company
            </t>
            <t t-else="else">
                <t t-set="company" t-value="res_company"/>
            </t>
        </t>
        <div t-attf-class="header o_company_#{company.id}_layout" t-att-style="report_header_style">
            <div class="row" height="50px">
                <div class="col-6 companycontract" name="company_address">
                    <div t-field="company.partner_id"
                        t-options='{"widget": "contact", "fields": ["name","address","mobile","phone","email"], "no_marker": true}'
                    />
                </div>
                <div class="col-6" name="company_name">
<!--                        <div class="col-7" style="margin-top:22px;font-size: 20px; font-weight: bold;font-family: Arial Narrow;" t-field="company.name" name="moto"/>-->
                    <div class="col-12">
                        <img t-if="company.logo" t-att-src="image_data_uri(company.logo)" width="200" height="100" alt="Logo" align="right"/>
                    </div>
                </div>
            </div>
            <t t-raw="0"/>
        </div>
    </template>

