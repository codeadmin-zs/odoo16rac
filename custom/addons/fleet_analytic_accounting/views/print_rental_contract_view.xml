    <template id="report_rental_contract">
        <t t-call="web.html_container">
            <t t-set="lang" t-value="docs.partner_id.lang or docs.company_id.partner_id.lang"/>
            <t t-call="fleet_analytic_accounting.print_rental_contract_document" t-lang="lang"/>
        </t>
    </template>
